<p id="notice"><%= notice %></p>

<h1>Listing Clinic Cr Conditions</h1>

<table>
  <thead>
    <tr>
      <th>Clinic cr doc</th>
      <th>Type</th>
      <th>Clinic condition</th>
      <th>Detail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clinic_cr_conditions.each do |clinic_cr_condition| %>
      <tr>
        <td><%= clinic_cr_condition.clinic_cr_doc %></td>
        <td><%= clinic_cr_condition.type %></td>
        <td><%= clinic_cr_condition.clinic_condition %></td>
        <td><%= clinic_cr_condition.detail %></td>
        <td><%= link_to 'Show', clinic_cr_condition %></td>
        <td><%= link_to 'Edit', edit_clinic_cr_condition_path(clinic_cr_condition) %></td>
        <td><%= link_to 'Destroy', clinic_cr_condition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

